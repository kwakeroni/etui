<?xml version="1.0" encoding="UTF-8"?>
<tag-collection xmlns="https://quaxantis.com/etui/tags" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://quaxantis.com/etui/tags tag-collection.xsd">
    <collection>Formats</collection>
    <family name="gif" group="GIF" readonly="true">
        <label>GIF</label>
        <reference>https://www.w3.org/Graphics/GIF/spec-gif89a.txt</reference>
        <description>
            Information extracted from GIF images.
        </description>
        <tags>
            <tag name="Comment" readonly="false" group="File" />
            <tag name="Duration" />
            <tag name="FrameCount" />
            <tag name="GIFVersion" />
            <tag name="Text" />
            <tag name="TransparentColor" />
            <tag name="MIDIControlVersion" />
            <tag name="SequenceNumber" />
            <tag name="MelodicPolyphony" />
            <tag name="PercussivePolyphony" />
            <tag name="ChannelUsage" />
            <tag name="DelayTime" />
            <tag name="AnimationIterations" />
            <tag name="ImageWidth" />
            <tag name="ImageHeight" />
            <tag name="HasColorMap" />
            <tag name="ColorResolutionDepth" />
            <tag name="BitsPerPixel" />
            <tag name="BackgroundColor" />
            <tag name="PixelAspectRatio" />
        </tags>
    </family>
    <family name="png" group="PNG">
        <label>PNG</label>
        <reference>http://www.libpng.org/pub/png/spec/1.2/</reference>
        <description>
            Tags extracted from PNG images.
        </description>
        <tags>
            <tag name="ImageWidth" readonly="true" />
            <tag name="ImageHeight" readonly="true" />
            <tag name="BitDepth" readonly="true" />
            <tag name="ColorType" readonly="true">
                <description>
                    0 = Grayscale
                    2 = RGB
                    3 = Palette
                    4 = Grayscale with Alpha
                    6 = RGB with Alpha
                </description>
            </tag>
            <tag name="Compression" readonly="true">
                <description>0 = Deflate/Inflate</description>
            </tag>
            <tag name="Filter" readonly="true">
                <description>0 = Adaptive</description>
            </tag>
            <tag name="Interlace" readonly="true">
                <description>
                    0 = Noninterlaced
                    1 = Adam7 Interlace
                </description>
            </tag>
            <tag name="PixelsPerUnitX" readonly="true" />
            <tag name="PixelsPerUnitY" readonly="true" />
            <tag name="PixelUnits" readonly="true" />
            <tag name="Artist" />
            <tag name="Author" />
            <tag name="Collection" />
            <tag name="Comment" />
            <tag name="Copyright" />
            <tag name="CreationTime" />
            <tag name="Description" />
            <tag name="Disclaimer" />
            <tag name="Document" />
            <tag name="Label" />
            <tag name="Make" />
            <tag name="Model" />
            <tag name="Software" />
            <tag name="Source" />
            <tag name="TimeStamp" />
            <tag name="Title" />
            <tag name="URL" />
            <tag name="PNGWarning" />
            <tag name="AestheticScore" />
            <tag name="CreateDate" />
            <tag name="ModDate" />
            <tag name="Parameters" />


            <tag name="FileName" readonly="true" />
            <tag name="Directory" readonly="true" />
            <tag name="FileSize" readonly="true" />
            <tag name="FileModifyDate" readonly="true" />
            <tag name="FileAccessDate" readonly="true" />
            <tag name="FileCreateDate" readonly="true" />
            <tag name="FilePermissions" readonly="true" />
            <tag name="FileType" readonly="true" />
            <tag name="FileTypeExtension" readonly="true" />
            <tag name="MIMEType" readonly="true" />
        </tags>
    </family>
    <family name="webp" group="RIFF" readonly="true">
        <label>WEBP</label>
        <tags>
            <tag name="AlphaPreprocessing" />
            <tag name="AlphaFiltering" />
            <tag name="AlphaCompression" />
            <tag name="BackgroundColor" />
            <tag name="AnimationLoopCount" />
            <tag name="Duration" />
            <tag name="VP8Version" />
            <tag name="ImageWidth" />
            <tag name="HorizontalScale" />
            <tag name="ImageHeight" />
            <tag name="VerticalScale" />
            <tag name="AlphaIsUsed" />
            <tag name="WebP_Flags" />
        </tags>
    </family>
    <family name="pdf" group="PDF">
        <label>PDF</label>
        <description>
            Tags extracted from PDF documents.
        </description>
        <!-- Note
             Custom tags can be added to the .ExifTool_config:
             ```
                %Image::ExifTool::UserDefined = (
                    'Image::ExifTool::PDF::Info' => {
                        MyTag => { },
                    },
                );
                1; # end
            ```
            The PDF:MyTag tag will then be visible in the Personalized section from Acrobat Reader.
         -->
        <tags>
            <tag name="PDFVersion" readonly="true" />
            <tag name="PageCount" readonly="true" />
            <tag name="PageLayout" readonly="true" />
            <tag name="PageMode" readonly="true" />
            <tag name="Language" readonly="true" />
            <tag name="Title" />
            <tag name="Author" />
            <tag name="Subject" />
            <tag name="Keywords" />
            <tag name="CreateDate" />
            <tag name="ModifyDate" />
            <tag name="SourceModified" />
            <tag name="Trapped" readonly="true"/>
            <tag name="Creator">
                <label>Creator Application</label>
                <description>Software application used to create the PDF</description>
            </tag>
            <tag name="Producer">
                <label>PDF Producer</label>
                <description>Software library that produced the PDF</description>
            </tag>
        </tags>
    </family>
</tag-collection>
